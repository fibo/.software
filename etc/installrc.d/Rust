
SOURCES_URI="not used"

function _build() {
	# Create profile
	#mkdir -p ~/.software/etc/Rust

	#cat <<-"PROFILE" > ~/.software/etc/Rust/profile
	#export PATH=$PATH:$DOTSOFTWARE_ROOT_DIR/opt/rust/bin
	#PROFILE

	curl -s https://static.rust-lang.org/rustup.sh | \
	sh -s -- \
	--yes \
	--disable-sudo \
	--channel=$RUST_VERSION \
	--prefix=$DOTSOFTWARE_ROOT_DIR
}


